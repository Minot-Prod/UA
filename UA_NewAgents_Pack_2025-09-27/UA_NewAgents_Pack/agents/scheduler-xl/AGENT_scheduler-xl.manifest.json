{
  "name": "Agent Scheduler XL",
  "slug": "scheduler-xl",
  "version": "1.0.0",
  "owner_token": "{{OWNER}}",
  "sector": "Content",
  "purpose": "Plan and queue content across platforms; maintain calendar alignment and collision avoidance.",
  "entrypoints": {
    "type": "prompt-agent",
    "routing_keywords": [
      "agent",
      "scheduler xl"
    ],
    "triggers": [
      "ua://scheduler-xl",
      "/agent scheduler-xl",
      "call agent scheduler xl"
    ]
  },
  "io_contract": {
    "inputs": [
      {
        "name": "context",
        "type": "object",
        "required": false,
        "desc": "Shared UA context/state."
      },
      {
        "name": "payload",
        "type": "object",
        "required": false,
        "desc": "Task-specific data."
      }
    ],
    "outputs": [
      {
        "name": "report",
        "type": "markdown",
        "desc": "Human-readable outcome and next steps."
      },
      {
        "name": "artifacts",
        "type": "list",
        "desc": "Files or structured results."
      },
      {
        "name": "logs",
        "type": "text",
        "desc": "Agent execution logs."
      }
    ]
  },
  "policies": {
    "base44": false,
    "login_first": true,
    "oauth_preferred": true,
    "safety": {
      "refuse_pii_exfiltration": true,
      "rate_limits": "respect external APIs"
    }
  },
  "capabilities": {
    "tools": [
      "web.run",
      "python_user_visible",
      "gmail.read",
      "gcal.read",
      "gcontacts.read"
    ],
    "integrations": [
      "Notion",
      "Instagram",
      "Facebook",
      "LinkedIn",
      "YouTube"
    ]
  },
  "tests": [
    {
      "id": "scheduler-xl-handshake",
      "type": "handshake",
      "given": "UA routes 'hello' to Agent Scheduler XL",
      "when": "agent starts",
      "then": "responds with its purpose and required inputs"
    },
    {
      "id": "scheduler-xl-routing",
      "type": "routing",
      "given": "UA dispatches task to scheduler-xl",
      "when": "agent completes",
      "then": "returns report+artifacts and updates registry"
    }
  ],
  "metadata": {
    "created": "2025-09-27",
    "labels": [
      "UA",
      "autodiscovery",
      "Content"
    ],
    "kpis": [
      "latency_ms",
      "success_rate",
      "interop_score"
    ]
  }
}