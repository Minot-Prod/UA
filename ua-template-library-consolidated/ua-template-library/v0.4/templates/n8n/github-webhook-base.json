{
  "name": "GitHub Webhook â†’ Process",
  "nodes": [
    {
      "parameters": {
        "path": "ua/github",
        "methods": [
          "POST"
        ]
      },
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "name": "GitHub Webhook"
    },
    {
      "parameters": {
        "functionCode": "return [{ json: {event: $json.headers['x-github-event'] || 'push', repo: $json.body?.repository?.full_name || ''} }];"
      },
      "type": "n8n-nodes-base.function",
      "typeVersion": 2,
      "name": "Extract Event"
    }
  ],
  "connections": {
    "GitHub Webhook": {
      "main": [
        [
          {
            "node": "Extract Event",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}