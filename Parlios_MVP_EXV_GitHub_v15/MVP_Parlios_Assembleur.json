{
  "meta": {
    "project": "Parlios MVP EXV",
    "version": "v15",
    "updated_at": "2025-10-04T05:09:46",
    "stage": 15,
    "stage_name": "Observabilité (logs, traces, métriques)"
  },
  "observability": {
    "analytics_opt_in_scope": "analytics",
    "session_cookie": "parlios.session",
    "session_fallback_local_key": "parlios.sid",
    "sampling": {
      "events": 1.0,
      "metrics": 1.0,
      "errors": 1.0
    },
    "buffer": {
      "max_events": 20,
      "flush_ms": 3000
    },
    "endpoints": {
      "ingest": "/api/telemetry",
      "export": "/api/telemetry/export",
      "health": "/api/telemetry/health"
    },
    "collectors": {
      "pageviews": true,
      "clicks": true,
      "errors": true,
      "web_vitals": [
        "LCP",
        "FID",
        "CLS"
      ]
    },
    "headers_forward": [
      "X-Parlios-Plan"
    ],
    "privacy": {
      "ip_masking": true,
      "strip_query_params": true,
      "fields_denylists": [
        "password",
        "token",
        "authorization"
      ]
    }
  }
}