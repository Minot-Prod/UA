    {
      "id": "agent.coordinator",
      "name": "Agent Coordinateur & Mémoire",
      "objective": "Orchestrer la Web Creation Team, router les tâches, maintenir la mémoire projet et résoudre les conflits entre agents.",
      "mode": {
        "beginner": true,
        "pro": true
      },
      "inputs": [
        {
          "id": "brief",
          "type": "object",
          "required": true,
          "desc": "Brief utilisateur (secteur, style, objectifs, exemples aimés)"
        },
        {
          "id": "prefs",
          "type": "object",
          "required": false,
          "desc": "Préférences UI (couleurs, ton, animations)"
        },
        {
          "id": "project_state",
          "type": "object",
          "required": false,
          "desc": "État courant (versions, validations, rejets)"
        }
      ],
      "outputs": [
        {
          "id": "plan",
          "type": "array",
          "desc": "Plan multi-étapes (agents ordonnés)"
        },
        {
          "id": "pack",
          "type": "object",
          "desc": "Livrables consolidés (tokens/pages/seo/medias/qa)"
        },
        {
          "id": "log",
          "type": "array",
          "desc": "Journal d’orchestration et décisions"
        }
      ],
      "capabilities": [
        "Routing sémantique par mots-clés & objectifs",
        "Mémoire épisodique (préférences, validations)",
        "Résolution de conflits (vote pondéré par domaine)",
        "Versioning + rollback",
        "Budget guardrails (stop 70%)"
      ],
      "actions": [
        "Construire un plan d’appel d’agents optimisé",
        "Fusionner les sorties en un pack cohérent",
        "Déclencher revues QA transverses"
      ],
      "tools": [
        {
          "name": "MemoryStore",
          "purpose": "Stocker préférences et historiques"
        },
        {
          "name": "Router",
          "purpose": "Appliquer règles de routage/ordre/dépendances"
        }
      ],
      "prompt_master": "Tu es le Coordinateur. Lis le brief, sélectionne l’ordre optimal d’agents, fixe des objectifs SMART par étape,\nsurveille les budgets et la cohérence. Résous les conflits (perf vs design, SEO vs copy) via un vote pondéré (Perf=0.9, SEO=0.8, UX=0.8, Brand=0.7).\nMaintiens une mémoire durable (goûts, validations) et fournis un pack final propre et traçable.",
      "evaluation": {
        "checks": [
          "Le plan couvre toutes les étapes (Strat→Brand→UX→Copy→Medias→Perf→SEO→CRO→Legal→QA)",
          "Aucun livrable contradictoire (tokens vs UI)",
          "Journal d’orchestration clair, décisions justifiées"
        ]
      },
      "metrics": {
        "planning_latency_ms": "<=150",
        "conflict_resolution_rate": ">=95%",
        "on_budget": ">=90% des runs"
      },
      "dependencies": [],
      "tags": [
        "core",
        "router",
        "memory"
      ]
    },
