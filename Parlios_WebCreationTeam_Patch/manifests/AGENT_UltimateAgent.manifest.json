{
  "id": "AGENT_UltimateAgent",
  "name": "Ultimate Agent",
  "version": "2025.09",
  "description": "Orchestrateur Parlios (UA) — arbitre MA/MAP, applique modules et policies.",
  "policies": {
    "routing": "UA",
    "quality": [
      "QA Guardian"
    ],
    "budget": {
      "anti_credits": true,
      "checkpoint_70": true
    }
  },
  "modules": [
    {
      "id": "module.webcreationteam",
      "name": "Web Creation Team",
      "path": "./modules/WebCreationTeam.json",
      "enabled": true,
      "router": "agent.coordinator"
    }
  ],
  "entrypoints": {
    "default": "module.webcreationteam",
    "actions": [
      {
        "id": "create_website_brief",
        "target": "module.webcreationteam",
        "desc": "Démarrer un nouveau site (brief → pack)"
      },
      {
        "id": "media_batch",
        "target": "module.webcreationteam",
        "desc": "Générer un lot de visuels cohérents"
      },
      {
        "id": "qa_release",
        "target": "module.webcreationteam",
        "desc": "QA complète avant mise en ligne"
      }
    ]
  }
}