{
  "name": "Driveâ†’Notion Index",
  "nodes": [
    {
      "parameters": {
        "operation": "list",
        "useQueryString": false,
        "filters": {}
      },
      "type": "n8n-nodes-base.googleDrive",
      "typeVersion": 2,
      "name": "List Files",
      "credentials": {
        "googleDriveOAuth2Api": {
          "id": "gdrive_cred_placeholder",
          "name": "Google Drive OAuth2"
        }
      }
    },
    {
      "parameters": {
        "operation": "create",
        "pageId": "={{$env.NOTION_DB_FILES}}",
        "propertiesUi": {
          "propertyFields": [
            {
              "key": "Name",
              "type": "title",
              "value": "={{$json.name}}"
            },
            {
              "key": "URL",
              "type": "url",
              "value": "={{$json.webViewLink || ''}}"
            },
            {
              "key": "Size",
              "type": "number",
              "value": "={{$json.size || 0}}"
            }
          ]
        }
      },
      "type": "n8n-nodes-base.notion",
      "typeVersion": 2,
      "name": "Notion Index",
      "credentials": {
        "notionApi": {
          "id": "notion_cred_placeholder",
          "name": "Notion OAuth"
        }
      }
    }
  ],
  "connections": {
    "List Files": {
      "main": [
        [
          {
            "node": "Notion Index",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}