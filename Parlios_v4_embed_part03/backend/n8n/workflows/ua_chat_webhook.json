{
  "name": "UA Chat Webhook",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "ua/chat",
        "responseMode": "onReceived",
        "options": {
          "responseData": "{\"ok\":true,\"intent\":\"{{$json.intent}}\"}",
          "responseCode": 200
        }
      },
      "id": "Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [
        300,
        300
      ]
    },
    {
      "parameters": {
        "rules": [
          {
            "conditions": {
              "string": [
                {
                  "value1": "={{$json.intent}}",
                  "operation": "equal",
                  "value2": "PING"
                }
              ]
            }
          },
          {
            "conditions": {
              "string": [
                {
                  "value1": "={{$json.intent}}",
                  "operation": "equal",
                  "value2": "DEMO"
                }
              ]
            }
          }
        ]
      },
      "id": "Switch",
      "type": "n8n-nodes-base.switch",
      "typeVersion": 2,
      "position": [
        550,
        300
      ]
    },
    {
      "parameters": {
        "responseBody": "={{ { \"ok\": true, \"intent\": $json.intent || \"UNKNOWN\" } }}",
        "responseCode": 200
      },
      "id": "ReturnJson",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1,
      "position": [
        800,
        300
      ]
    }
  ],
  "connections": {
    "Webhook": {
      "main": [
        [
          {
            "node": "Switch",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Switch": {
      "main": [
        [
          {
            "node": "ReturnJson",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "ReturnJson",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}